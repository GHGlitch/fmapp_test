        <client_script><![CDATA[angular.bootstrap($('angularApp'), ['FloorViewApp']);

function begin() {
	
	var data = getSourceData();
 	//console.log('>>>>> ' + JSON.stringify(data));
	
	var spaceScope = angular.element($("floorview-div")).scope();
	spaceScope.selectRoomFromOutlook(data.space);
	
	var scope = angular.element($("reservation-div")).scope();
	scope.selectedDate = new Date(data.selectedDate);
	scope.startHour = data.startHour;
	scope.startMin = data.startMin;
	scope.endHour = data.endHour;
	scope.endMin = data.endMin;
    scope.selectedLocationName = data.locationName;
    scope.selectedLocationID = data.location;
    scope.selectedFloorName = data.floorName;
    scope.selectedFloorID = data.floor;
    
    var ga = new GlideAjax('x_lsmcb_fm.OutlookProcessor');
    ga.addParam('sysparm_name', 'getLocations');
    ga.getXML(getResponse);

    function getResponse(response) {

        var answer = response.responseXML.documentElement.getAttribute("answer");
        answer = JSON.parse(answer);
        scope.locations = answer;
        scope.initialLoad();
        scope.validateTimes();
        //validateAndUpdate();
    }
}

begin();

function getFloorName(floor_sysid){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'getOnlyFloorName');
	ga.addParam('sysparm_floor_sysid' , floor_sysid);
	ga.getXML(nameResponse);

	function nameResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");

		$('sys_display.floor').value = answer;
	}
}


function getLocationName(location_sysid){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'getOnlyLocationName');
	ga.addParam('sysparm_loc_sysid' , location_sysid);
	ga.getXML(nameResponse);

	function nameResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");

		$('sys_display.location').value = answer;

	}
}

function floorFill(location_sysid){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'checkOnlyFloor');
	ga.addParam('sysparm_loc_sysid' , location_sysid);
	ga.getXML(floorResponse);

	function floorResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");

		if(!answer) return alert('Error fetching Floor Data, or there are no Floors available!');	
		var floorArray = JSON.parse(answer);

		if (floorArray.length == '1') {
			var onlyFloor = floorArray[0].toString();
			gel('floor').setValue(onlyFloor);
			getFloorName(onlyFloor);
			validateAndUpdate();
		} else {
			$('sys_display.floor').value = '';
			gel('floor').setValue('');
		}
	}
}


function isOnlyOne(){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'checkOnlyLocation');
	ga.getXML(locationResponse);

	function locationResponse(response){
		
		var answer = response.responseXML.documentElement.getAttribute("answer");

		var locationArray = JSON.parse(answer);
		floorFill(gel('location').value);

		if (locationArray.length == '1') {
			var onlyLocation = locationArray[0].toString();
			gel('location').setValue(onlyLocation);
			getLocationName(onlyLocation);
			floorFill(onlyLocation);

		}
	}
}


function checkIfEdgeOrIE() {
	
	// Verify if the current browser possesses a native datepicker by creating an input element of the type 'date'.
	// After, check if the type changed to 'text' - if so, the browser is IE or Edge.
	var elem = document.createElement('input');
	elem.setAttribute('type', 'date');
 
	if (elem.type === 'text') {
		return true;
	}
	
	return false;
}

if (checkIfEdgeOrIE()) {
    $j('#date').datepicker({
        dateFormat: 'yy-mm-dd',
        minDate: 0,
        changeMonth: true,
        changeYear: true,
        firstDay: 1
    });
	
	// Enforce that this field is NOT editable in any way other than using the datepicker
	$j("#date").on('keydown',  function(e){
		var key = e.charCode || e.keyCode || e.which;
		if( key === 8 || key=== 46 ){
			e.preventDefault();
			return false;
		}
	});
	
	$j('#date').keypress(function(e) {
		return false;
	});
}]]></client_script>
