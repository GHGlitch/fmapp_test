<table width="200" border="1" cellspacing="3" cellpadding="3" class="meeting_table" ng-controller="MeetingDetailsController" ng-cloak="">
  <tbody>
    <tr>
      <td valign="top">
     	<table width="94%" border="0" cellspacing="5" cellpadding="5" bgcolor="#eee" align="center" style="margin:15px">
  			<tbody>
   				 <tr>
     				 <td width="110px" bgcolor="#666464"><div class="meeting_title">MEETING DETAILS</div></td>
     				 <td valign="middle" bgcolor="#666464"><div id="meeting_title_strip"></div></td>
   				 </tr>
  			</tbody>
		</table>
</td>
    </tr>
    <tr>
      <td valign="top">
      <table width="165" border="0" cellspacing="5" cellpadding="5" class="meetingDetails_table " align="center"  ng-show="isResSelected">
 		 <tbody>
   			 <tr>
     			 <td valign="top"><div class="meetingDetails_title">{{ roomData.roomInfo.name || 'Select a room.' }}</div></td>
   			 </tr>
   			 <tr>
    			  <td valign="top"><span class="meetingDetails_desctitle">Requester: </span><span class="meetingDetails_decvalue">{{ selectedReservation.requester }}</span></td>
   			 </tr>
			 <tr>
    			  <td valign="top"><span class="meetingDetails_desctitle">Subject: </span><span class="meetingDetails_decvalue">{{ selectedReservation.meeting_subject }}</span></td>
   			 </tr>
			 <tr>
    			  <td valign="top"><span class="meetingDetails_desctitle">Date: </span><span class="meetingDetails_decvalue">{{ selectedReservation.startTime.substring(0,11) }}</span></td>
   			 </tr>
			  <tr>
    			  <td valign="top"><span class="meetingDetails_desctitle">Start Time: </span><span class="meetingDetails_decvalue">{{ selectedReservation.startTime.substring(11,16) }}</span></td>
   			 </tr>
			 <tr>
    			  <td valign="top"><span class="meetingDetails_desctitle">End Time: </span><span class="meetingDetails_decvalue">{{ selectedReservation.endTime.substring(11,16) }}</span></td>
   			 </tr>
   			 <tr>
				 <td valign="top"><!-- <div class="roomprops_shortdesc">Short description, like lorem ipsum dolor sit amet.</div> --></td>
   			 </tr>
 		 </tbody>
	</table>

      </td>
    </tr>
  </tbody>
</table>

