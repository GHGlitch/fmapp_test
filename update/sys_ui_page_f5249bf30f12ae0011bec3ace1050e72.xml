        <client_script><![CDATA[angular.bootstrap($('angularApp'), ['FloorViewApp']);


function getFloorName(floor_sysid){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'getOnlyFloorName');
	ga.addParam('sysparm_floor_sysid' , floor_sysid);
	ga.getXML(nameResponse);

	function nameResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");

		$('sys_display.floor').value = answer;
	}
}


function getLocationName(location_sysid){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'getOnlyLocationName');
	ga.addParam('sysparm_loc_sysid' , location_sysid);
	ga.getXML(nameResponse);

	function nameResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");

		$('sys_display.location').value = answer;

	}
}

function floorFill(location_sysid){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'checkOnlyFloor');
	ga.addParam('sysparm_loc_sysid' , location_sysid);
	ga.getXML(floorResponse);

	function floorResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");

		if(!answer) return alert('Error fetching Floor Data, or there are no Floors available!');	
		var floorArray = JSON.parse(answer);

		if (floorArray.length == '1') {
			var onlyFloor = floorArray[0].toString();
			gel('floor').setValue(onlyFloor);
			getFloorName(onlyFloor);
			validateAndUpdate();
		} else {
			$('sys_display.floor').value = '';
			gel('floor').setValue('');
		}
	}
}


function isOnlyOne(){

	var ga = new GlideAjax ('x_lsmcb_fm.FMRoomReservationAjaxUtils');
	ga.addParam('sysparm_name' , 'checkOnlyLocation');
	ga.getXML(locationResponse);

	function locationResponse(response){
		
		var answer = response.responseXML.documentElement.getAttribute("answer");

		var locationArray = JSON.parse(answer);
		floorFill(gel('location').value);

		if (locationArray.length == '1') {
			var onlyLocation = locationArray[0].toString();
			gel('location').setValue(onlyLocation);
			getLocationName(onlyLocation);
			floorFill(onlyLocation);

		}
	}
}

isOnlyOne();


]]></client_script>
